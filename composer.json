{
    "name": "devsolux/laravel-generator",
    "type": "library",
    "description": "Laravel Generator",
    "keywords": [],
    "license": "MIT",
    "authors": [{
        "name": "Baris Aydin",
        "email": "devsolux@gmail.com"
    }],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "laracasts/flash": "^3.2.2",
        "laravelcollective/html": "^6.4",
        "symfony/var-exporter": "^6.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.7",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^8.0.0",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-laravel": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "DevSolux\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\DevSolux\\Generator\\GeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
